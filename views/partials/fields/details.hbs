<div class="d details-wrap">
    <table class=details>
        <tr>
            <th>Key isotopes</th>
            <td>{{formatIsotopes element.keyIsotopes element.symbol}}</td>
        </tr>
        <tr>
            <th>Electron configuration</th>
            <td>{{formatElectronConfig element.electronConfiguration}}</td>
        </tr>
        <tr>
            <th>Density (g/mL)</th>
            <td>{{element.density}}</td>
        </tr>
        <tr>
            <th>Ionisation energy (kJ/mol)</th>
            <td>{{element.ionizationEnergy}}</td>
        </tr>
    </table>
</div>
